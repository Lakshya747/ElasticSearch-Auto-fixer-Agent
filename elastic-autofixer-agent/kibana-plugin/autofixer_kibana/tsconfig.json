{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "kibana/server": ["./kibana.d.ts"],
      "kibana/public": ["./kibana.d.ts"]
    }
  },
  "include": ["server/**/*", "public/**/*", "common/**/*", "kibana.d.ts"],
  "exclude": ["node_modules"]
}